<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="RedButton" parent="ThemeOverlay.AppCompat.Light">
        <item name="colorAccent">@color/c_red_munsell</item>
    </style>

    <style name="WhiteButton" parent="ThemeOverlay.AppCompat.Light">
        <item name="colorAccent">@color/c_white</item>
        <item name="android:textColor">@color/c_blue_crayola</item>
    </style>

</resources>
